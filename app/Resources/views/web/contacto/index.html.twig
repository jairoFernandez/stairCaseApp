{% extends 'web/dashboard.html.twig' %}

{% block body %}
    <script>
        localStorage.setItem("menuLateral", "admin-contactos");
    </script> 
    <h1>Contacto list</h1>
    <div class="col-lg-4">
        <a href="{{ path('admin_new') }}">Crear nuevo</a>
    <div class="list-group">
        {% for contacto in contactos %}
            <a href="{{ path('admin_show', { 'id': contacto.id  }) }}" class="list-group-item">  <span class="badge">{{ contacto.telefono }}</span> {{ contacto.primerNombre }} {{ contacto.primerApellido }}</a>
        {% endfor %}
    </div>

    </div>
{% endblock %}
